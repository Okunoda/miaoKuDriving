page {
  background-color: #eef0f6;
  height: 100%;
  font-family: Helvetica Neue, Helvetica, Arial, Microsoft Yahei, Hiragino Sans GB, Heiti SC, WenQuanYi Micro Hei, sans-serif;
}
.page {
  padding-bottom: 70 rpx;
  padding-left: 40 rpx;
  padding-right: 40 rpx;
  padding-top: 120 rpx;
}
.notice {
  background-color: #fee7a3;
  position: fixed;
  z-index: 999;
  top: 0;
  left: 0;
  width: 100%;
  padding: 20 rpx 30 rpx;
}
.notice text {
  font-size: 26 rpx;
  color: #ac9146;
  margin-left: 10 rpx;
}
.credentials-container {
  display: flex;
  justify-content: space-between;
  flex-wrap: wrap;
}
.credentials-container .credentials {
  width: 214 rpx;
  height: 150 rpx;
  margin-bottom: 60 rpx;
  position: relative;
}
.credentials-container .credentials .bg {
  width: 214 rpx;
  height: 150 rpx;
  display: block;
  border-radius: 10 rpx;
}
.credentials-container .credentials .cover {
  position: absolute;
  left: 0;
  top: 0;
  width: 214 rpx;
  height: 150 rpx;
}
.credentials-container .credentials .cover .card {
  width: 46 rpx;
  margin-top: 20 rpx;
  margin-left: 25 rpx;
}
.credentials-container .credentials .cover .desc {
  display: block;
  font-size: 24 rpx;
  color: #fff;
  margin-left: 25 rpx;
  margin-top: 0 rpx;
  line-height: 1;
  font-weight: bold;
}
.credentials-container .credentials .cover .camera {
  border: none;
  background-color: #2074fd;
  width: 70 rpx;
  height: 70 rpx;
  border-radius: 35 rpx;
  margin-left: auto;
  margin-right: auto;
  margin-top: 15 rpx;
  background-image: url(data:image/png;base64,);
  background-size: 40 rpx;
  background-repeat: no-repeat;
  background-position: 16 rpx 15 rpx;
}
.credentials-container .credentials .cover .camera:active {
  width: 70 rpx;
  height: 70 rpx;
  border-radius: 35 rpx;
  background-color: #0259e9;
}
.title {
  font-weight: bold;
  font-size: 32 rpx;
  color: #444;
  margin-top: 25 rpx;
  margin-bottom: 25 rpx;
  padding-left: 10 rpx;
}
.list {
  border-radius: 20 rpx;
  background-color: #fff;
  overflow: hidden;
}
.btn {
  background-color: #2074fd;
  color: #fff;
  margin-top: 40 rpx;
  border: none;
  padding-top: 8 rpx;
  padding-bottom: 8 rpx;
}
.btn::after {
  border: none;
}
.btn:active {
  background-color: #0259e9;
}
.remark {
  color: #a0a0a0;
  font-size: 28 rpx;
  margin-top: 30 rpx;
  line-height: 1.7;
  bottom: 60 rpx;
}

