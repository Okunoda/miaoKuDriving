page {
  font-family: Helvetica Neue, Helvetica, Arial, Microsoft Yahei, Hiragino Sans GB, Heiti SC, WenQuanYi Micro Hei, sans-serif;
}
.page {
  padding: 50 rpx 40 rpx 70 rpx 40 rpx;
}
.item {
  display: flex;
  font-size: 32 rpx;
  color: #444;
  line-height: 1;
  margin-top: 40 rpx;
  margin-bottom: 20 rpx;
}
.item::before {
  content: '';
  display: block;
  width: 10 rpx;
  height: 32 rpx;
  background-color: #777;
  margin-right: 15 rpx;
  border-radius: 3 rpx;
}
.desc {
  font-size: 28 rpx;
  color: #a0a0a0;
  line-height: 1.7;
  padding-left: 25 rpx;
  padding-top: 10 rpx;
  padding-bottom: 10 rpx;
  display: flex;
}
.desc .num {
  margin-right: 10 rpx;
}

